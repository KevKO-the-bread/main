liste=[4,2,5,6,3,1]


class Solution(object):

    def sortArray(self, nums):
        for i in range (1,len(nums)):
            j=i-1
            while j>=0 and nums[j]<nums[j+1]:
                temp=nums[j+1]
                nums[j+1]=nums[j]
                nums[j]=temp
                j-=1
        return nums
        
print(Solution().sortArray(liste))
      
